    <commands name="GetVersionInfo" description="List with version strings of all entities modelled by this device&#xA;&#xA;For each entity, a JSON string containing the following information:&#xA;     entityName: An enumeration to indicate the type of the entity. &#xA;                         Possible set of values {Subelement, Assembly, &#xA;                         Subassembly, Component}&#xA;     entityType: An enumeration to indicate the type of the entity. &#xA;                       Possible set of values {Subelement, Assembly, &#xA;                       Subassembly, Component}&#xA;     Software/Firmware Info: A string to indicate the software/firmware&#xA;                                         version indicating major and minor &#xA;                                         version &#xA;    installationTimeStamp: Time at which software/firmware was&#xA;                                        installed." execMethod="get_version_info" displayLevel="OPERATOR" polledPeriod="0">
      <argin description="">
        <type xsi:type="pogoDsl:VoidType"/>
      </argin>
      <argout description="">
        <type xsi:type="pogoDsl:StringArrayType"/>
      </argout>
      <status abstract="false" inherited="true" concrete="true"/>
    </commands>
    <commands name="log" description="This is the standard log target if redirecting logging to a device. The&#xA;options for Tango logging are:&#xA;&#xA;1) Console&#xA;2) File&#xA;3) Other device&#xA;&#xA;The Tango documentation talks mostly about the supplied GUI &#xA;`LogViewer` - but the example file Logtest.py (in this directory) shows&#xA;a more gneral approach&#xA;&#xA;I beleive that the limitation that the logging level applies&#xA;to all log targets still exists" execMethod="log" displayLevel="OPERATOR" polledPeriod="0" isDynamic="false">
      <argin description="# Tango Manual Appendix 9 gives the format&#xA;        # argin[0] = millisecond Unix timestamp&#xA;        # argin[1] = log level&#xA;        # argin[2] = the source log device name&#xA;        # argin[3] = the log message&#xA;        # argin[4] = Not used - reserved&#xA;        # argin[5] = thread identifier of originating message">
        <type xsi:type="pogoDsl:StringArrayType"/>
      </argin>
      <argout description="">
        <type xsi:type="pogoDsl:VoidType"/>
      </argout>
      <status abstract="false" inherited="false" concrete="true" concreteHere="true"/>
    </commands>
