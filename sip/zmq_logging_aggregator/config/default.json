{
  "version": 1,
  "disable_existing_loggers": false,

  "loggers": {
   "": {
     "level": "DEBUG",
     "handlers": ["console"]
   }
  },

  "handlers":{
    "console": {
      "class": "logging.StreamHandler",
      "level": "DEBUG",
      "formatter": "format1",
      "stream": "ext://sys.stdout"
    }
  },

  "formatters": {
    "format1": {
     "format": "> %(name).40s | %(levelname).6s | %(message)s"
    }
  }
}
